generator client {
  provider = "prisma-client-js"
  output   = "../../../node-module/@prisma-clients/jobberr-auth"
}

datasource db {
  provider = "postgresql"
  url      = env("AUTH_DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  passwoed String
}
